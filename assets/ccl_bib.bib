}

@article{chandramouliWorkflowBuildingComputationally2024,
  title = {A {{Workflow}} for {{Building Computationally Rational Models}} of {{Human Behavior}}},
  author = {Chandramouli, Suyog and Shi, Danqing and Putkonen, Aini and De Peuter, Sebastiaan and Zhang, Shanshan and Jokinen, Jussi and Howes, Andrew and Oulasvirta, Antti},
  year = {2024},
