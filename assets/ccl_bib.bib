  file = {/Users/thomasgorman/Zotero/storage/N5VKGTPR/Wang et al. - 2024 - Evaluating and Modeling Social Intelligence A Com.pdf;/Users/thomasgorman/Zotero/storage/T238VITW/2405.html}
