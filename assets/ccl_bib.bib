@article{matzPotentialGenerativeAI2024,
  title = {The Potential of Generative {{AI}} for Personalized Persuasion at Scale},
  author = {Matz, S. C. and Teeny, J. D. and Vaid, S. S. and Peters, H. and Harari, G. M. and Cerf, M.},
