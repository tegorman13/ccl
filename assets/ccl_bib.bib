  author = {Rivkin, Dmitriy and Hogan, Francois and Feriani, Amal and Konar, Abhisek and Sigal, Adam and Liu, Xue and Dudek, Gregory},
  year = {2024},
  journal = {IEEE Internet of Things Journal},
