@article{kammerEffectsInteractingLarge2024,
  title = {Effects of Interacting with a Large Language Model Compared with a Human Coach on the Clinical Diagnostic Process and Outcomes among Fourth-Year Medical Students: Study Protocol for a Prospective, Randomised Experiment Using Patient Vignettes},
  shorttitle = {Effects of Interacting with a Large Language Model Compared with a Human Coach on the Clinical Diagnostic Process and Outcomes among Fourth-Year Medical Students},
  author = {K{\"a}mmer, Juliane E and Hautz, Wolf E and Krummrey, Gert and Sauter, Thomas C and Penders, Dorothea and Birrenbach, Tanja and Bienefeld, Nadine},
  year = {2024},
