  file = {/Users/thomasgorman/Zotero/storage/2ZEL5CK6/Rey-Jouanchicot et al. - 2024 - Leveraging Large Language Models for enhanced personalised user experience in Smart Homes.pdf;/Users/thomasgorman/Zotero/storage/VW7QE4BG/2407.html}
