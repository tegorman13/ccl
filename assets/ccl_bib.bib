@misc{leporiDoorsPerceptionVision2024,
  title = {Beyond the {{Doors}} of {{Perception}}: {{Vision Transformers Represent Relations Between Objects}}},
  shorttitle = {Beyond the {{Doors}} of {{Perception}}},
