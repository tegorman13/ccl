@misc{huangLVLMsUnderstandCharts2023,
  title = {Do {{LVLMs Understand Charts}}? {{Analyzing}} and {{Correcting Factual Errors}} in {{Chart Captioning}}},
  shorttitle = {Do {{LVLMs Understand Charts}}?},
